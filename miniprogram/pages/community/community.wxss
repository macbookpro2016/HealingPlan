.community-container {
  padding: 20rpx;
}
.tab-bar {
  display: flex;
  justify-content: space-around;
  margin-bottom: 20rpx;
}
.tab-item {
  padding: 10rpx;
  border-bottom: 2px solid transparent;
}
.active {
  color: #FF6B6B;
  border-bottom-color: #FF6B6B;
}
.post {
  margin-bottom: 20rpx;
  padding: 10rpx;
  border: 1px solid #ccc;
  border-radius: 10rpx;
  background-color: #ffffff;
}
.interact {
  display: flex;
  justify-content: space-around;
  margin-top: 10rpx;
}