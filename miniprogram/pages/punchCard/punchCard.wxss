.punchCard-container {
  padding: 20rpx;
}
.punch-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx;
  border-bottom: 1px solid #ccc;
}
.reward {
  margin-top: 20rpx;
  padding: 20rpx;
  border: 1px solid #ccc;
  border-radius: 10rpx;
  background-color: #f9f9f9;
  text-align: center;
}
.reward image {
  width: 100rpx;
  height: 100rpx;
  margin: 10rpx auto;
}